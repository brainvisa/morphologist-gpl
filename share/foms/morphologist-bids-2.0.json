{
    "fom_name": "morphologist-bids-2.0",
    "fom_import": [
        "brainvisa-formats-3.2.0",
        "shared-brainvisa-1.0",
        "raw-bids-1.0"
    ],
    "attribute_definitions": {
        "session": {
            "default_value": "0"
        },
        "analysis": {
            "default_value": "0"
        },
        "graph_version": {
            "default_value": "3.1"
        },
        "sulci_recognition_session": {
            "default_value": "0"
        },
        "bids": {
            "default_value": "run-0"
        }
    },
    "shared_patterns": {
      "acquisition": "sub-<subject>/ses-<session>/<bids>",
      "analysis": "{acquisition}/ana-<analysis>",
      "sub": "sub-<subject>_ses-<session>_<bids>",
      "reg": "{acquisition}/registration",
      "seg": "{analysis}/segmentation",
      "mesh": "{analysis}/mesh",
      "fold": "{analysis}/folds/<graph_version>",
      "foldauto": "{fold}/sul-<sulci_recognition_session>_auto",
      "fautosub": "{sub}_sul-<sulci_recognition_session>"
    },

    "processes": {
        "Morphologist": {
            # t1mri should be defined through the import process
            "t1mri": [
                [
                    "input:{acquisition}/{sub}_T1w",
                    "Aims readable volume formats"
                ],
                [
                    "input:sub-<subject>/ses-<session>/{sub}_T1w",
                    "Aims readable volume formats"
                ],
                [
                    "input:sub-<subject>/ses-<session>/sub-<subject>_ses-<session>_T1w",
                    "Aims readable volume formats"
                ],
                [
                    "input:{acquisition}/sub-<subject>_ses-<session>_T1w",
                    "Aims readable volume formats",
                ]
            ],
            "imported_t1mri": [
                [
                    "output:{acquisition}/{sub}_T1w",
                    "Aims readable volume formats"
                ]
            ],
            "reoriented_t1mri":
                [["output:{acquisition}/{sub}_T1w", "Aims writable volume formats"]],
            #### NormalizeSPM
            "normalization_spm_native_transformation_pass1": [["output:{acquisition}/{sub}_sn_pass1", "Matlab file"]],

            "t1mri_nobias": [
                [
                    "output:{seg}/{sub}_nobias",
                    "Aims writable volume formats"
                ]
            ],
            "commissure_coordinates": [
                [
                    "output:{acquisition}/{sub}",
                    [
                        "Commissure coordinates"
                    ]
                ]
            ],
            "histo_analysis": [
                [
                    "output:{seg}/{sub}_nobias",
                    [
                        "Histo Analysis"
                    ]
                ]
            ],
            "split_brain": [
                [
                    "output:{seg}/{sub}_voronoi",
                    "Aims writable volume formats"
                ]
            ],
            "left_graph": [
                [
                    "output:{fold}/{sub}_L",
                    [
                        "Graph and data"
                    ]
                ]
            ],
            "right_graph": [
                [
                    "output:{fold}/{sub}_R",
                    [
                        "Graph and data"
                    ]
                ]
            ],
            "left_labelled_graph": [
                [
                    "output:{foldauto}/{fautosub}_L_auto",
                    [
                        "Graph and data"
                    ]
                ]
            ],
            "right_labelled_graph": [
                [
                    "output:{foldauto}/{fautosub}_R_auto",
                    [
                        "Graph and data"
                    ]
                ]
            ],

            ### TalairachFromNormalization
            "PrepareSubject_TalairachFromNormalization_transform_chain_ACPC_to_Normalized": "{shared.talairach_TO_spm_transform}",

            ## CNN_recognition19
            #  (need to be forced because the generic process needs a side)
            "SulciRecognition_CNN_recognition19_model_file":
                [["shared:models/models_2019/cnn_models/sulci_unet_model_left",
                  "Deep model file", {"side": "L"}]],
            "SulciRecognition_1_CNN_recognition19_model_file":
                [["shared:models/models_2019/cnn_models/sulci_unet_model_right",
                  "Deep model file", {"side": "R"}]],
            "SulciRecognition_CNN_recognition19_param_file":
                [["shared:models/models_2019/cnn_models/sulci_unet_model_params_left",
                  "JSON file", {"side": "L"}]],
            "SulciRecognition_1_CNN_recognition19_param_file":
                [["shared:models/models_2019/cnn_models/sulci_unet_model_params_right",
                  "JSON file", {"side": "R"}]],

        },
        "Morphologist.PrepareSubject": {
            "commissure_coordinates": [
                [
                    "output:{acquisition}/{sub}",
                    [
                        "Commissure coordinates"
                    ]
                ]
            ],
            "Normalization_NormalizeSPM_template": "{shared.anatomical_template_t1}",
