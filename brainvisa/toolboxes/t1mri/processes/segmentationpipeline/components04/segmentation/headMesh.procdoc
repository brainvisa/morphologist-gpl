<minf version="1.0">
  <d name="fr" length="3">
    <xhtml name="short"/>
    <d name="parameters" length="8">
      <xhtml name="keep_head_mask"/>
      <xhtml name="first_slice"/>
      <xhtml name="head_mesh"/>
      <xhtml name="head_mask"/>
      <xhtml name="mri_corrected"/>
      <xhtml name="threshold"/>
