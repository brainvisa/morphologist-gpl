<d>
  <d name="fr">
    <xhtml name="short"/>
    <d name="parameters">
      <xhtml name="graph_version"/>
      <xhtml name="database"/>
      <xhtml name="keys"/>
      <xhtml name="output_file"/>
      <xhtml name="analysis"/>
